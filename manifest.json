{
    "manifest_version": 2,
    "name": "Steam Pixel",
    "version": "0.2",
  
    "description": "Adds Open In Steam, SteamDB and ProtonDB buttons to the Steam Website.",
  
    "icons": {
      "48": "assets/steam-48.png"
    },
  
    "content_scripts": [
      {
        "matches": ["*://*.steampowered.com/app/*"],
        "js": ["steamPixel.js"]
      }
    ]
  }